<div class="mx-auto md:w-3/4 w-full">
  <div class="flex justify-between items-center">
    <h1 class="font-bold text-4xl">新規予約</h1>
    <%= link_to '予約トップへ戻る', occupations_path, 
        class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
  </div>

  <div id="new-occupation" class="mt-5 mb-10">
    <new-occupation tday="<%= @tday %>" 
                    :mns="<%= @mns %>" 
                    :mnlist="<%= @mnlist.to_json %>" 
                    :current="<%= current_user.id %>">
    </new-occupation>
  </div>

</div>




<% content_for :meta_t do %>
<meta name="turbo-visit-control" content="reload">
<% end %>
